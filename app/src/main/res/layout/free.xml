<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear9"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/textview1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="Infinity Patcher  "
			android:textSize="18sp"
			android:textStyle="bold"
			android:textColor="#4CAF50"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/textview2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Choose Your Version :"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#3F51B5"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear5"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="horizontal">
		<Switch
			android:id="@+id/gl"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Global"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#9C27B0"/>
		<Switch
			android:id="@+id/kr"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Korea"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#795548"/>
		<Switch
			android:id="@+id/lite"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Lite"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#03A9F4"/>
		<Button
			android:id="@+id/reset"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Reset"
			android:textSize="12sp"
			android:textColor="#000000"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear2"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/textview3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Choose Your Hacks :"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#3F51B5"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear3"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="horizontal">
		<CheckBox
			android:id="@+id/fps"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="90 FPS"
			android:textSize="12sp"
			android:textColor="#000000"/>
		<CheckBox
			android:id="@+id/grass"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="No Grass"
			android:textSize="12sp"
			android:textColor="#000000"/>
		<CheckBox
			android:id="@+id/sky"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Black Sky + No Fog"
			android:textSize="12sp"
			android:textColor="#000000"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear4"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="horizontal">
		<CheckBox
			android:id="@+id/ipad"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="IPAD View"
			android:textSize="12sp"
			android:textColor="#000000"/>
		<CheckBox
			android:id="@+id/fog"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="No Fog"
			android:textSize="12sp"
			android:textColor="#000000"/>
		<CheckBox
			android:id="@+id/magic_bullet"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Aim Assist (High)"
			android:textSize="12sp"
			android:textColor="#000000"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear6"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="horizontal">
		<CheckBox
			android:id="@+id/zero"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Zero Gravity "
			android:textSize="12sp"
			android:textColor="#000000"/>
		<CheckBox
			android:id="@+id/jump"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Jump Float"
			android:textSize="12sp"
			android:textColor="#000000"/>
		<CheckBox
			android:id="@+id/less_recoil"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Less Recoil"
			android:textSize="12sp"
			android:textColor="#000000"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear10"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="horizontal">
		<CheckBox
			android:id="@+id/damage"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="High Damage"
			android:textSize="12sp"
			android:textColor="#000000"/>
		<CheckBox
			android:id="@+id/manual"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Manual Mode"
			android:textSize="12sp"
			android:textColor="#000000"/>
	</LinearLayout>
	<ProgressBar
		android:id="@+id/progressbar1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:indeterminate="false"
		style="?android:progressBarStyle"/>
	<Button
		android:id="@+id/start"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:text="Apply &amp; Start Game"
		android:textSize="12sp"
		android:textColor="#000000"/>
	<LinearLayout
		android:id="@+id/linear8"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="horizontal"/>
	<Button
		android:id="@+id/end"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:text="Restore All"
		android:textSize="12sp"
		android:textColor="#000000"/>
	<TextView
		android:id="@+id/textview4"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:text="Developer : \@Rhythm113"
		android:textSize="12sp"
		android:textStyle="bold"
		android:textColor="#009688"/>
	<TextView
		android:id="@+id/textview5"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:text="Â© Infinity Creators"
		android:textSize="12sp"
		android:textStyle="bold"
		android:textColor="#F44336"/>
</LinearLayout>
